<?xml version="1.0" encoding="utf-8"?>
<root>
	
	<input_action id="PhotoModeToggle" scope="MainGameOnly"
      category="PhotoModeCamera" display_name="Photo Mode Toggle"
      description="Enter or exit photo mode."
      check_type="Custom" allowed_in_modes="Normal,MapMode,BuildMode,CommandMode,PhotoMode,EndOfTime,Hacking,ZodiacPods,ZodiacNeurons,ZodiacHexes">
		<keybind id="4" key="F2" />
		<keybind id="5" key="" />
	</input_action>

	<input_action id="PhotoModeManualFocus" scope="MainGameOnly"
      category="PhotoModeCamera" display_name="Hold For Manual Focus"
      description="Hold down and move the mouse to manually focus the camera."
      check_type="Custom" allowed_in_modes="PhotoMode" >
		<keybind id="1" key="M" />
		<keybind id="2" key="" />
	</input_action>

	<input_action id="PhotoModeTakePhoto" scope="MainGameOnly"
      category="PhotoModeCamera" display_name="Take Photo"
      description="Press to take a photo."
      check_type="Custom" allowed_in_modes="PhotoMode" >
		<keybind id="1" key="Space" />
		<keybind id="2" key="None" />
	</input_action>
	
	<input_action id="PhotoModeCameraSpeedUp" scope="MainGameOnly"
      category="PhotoModeCamera" display_name="Camera Speed Up"
      description="Make all camera movements faster."
      check_type="Custom" allowed_in_modes="PhotoMode" >
		<keybind id="1" key="LeftShift" />
		<keybind id="2" key="" />
	</input_action>
	
	<input_action id="PhotoModeCameraSlowDown" scope="MainGameOnly"
		category="PhotoModeCamera" display_name="Camera Slow Down"
		description="Make all camera movements slower."
		check_type="Custom" allowed_in_modes="PhotoMode" >
		<keybind id="1" key="LeftControl" />
		<keybind id="2" key="" />
	</input_action>
	
	<input_action id="PhotoModeCameraForward"
		category="PhotoModeCamera" display_name="Camera Forward" scope="MainGameOnly"
		description="Pan the camera forward."
		check_type="Custom" allowed_in_modes="PhotoMode" >
		<keybind id="1" key="W" />
		<keybind id="2" key="UpArrow" />
	</input_action>
	
	<input_action id="PhotoModeCameraBackward" scope="MainGameOnly"
		category="PhotoModeCamera" display_name="Camera Backward"
		description="Pan the camera backward."
		check_type="Custom" allowed_in_modes="PhotoMode">
		<keybind id="1" key="S" />
		<keybind id="2" key="DownArrow" />
	</input_action>

	<input_action id="PhotoModeCameraLeft" scope="MainGameOnly"
		category="PhotoModeCamera" display_name="Camera Left"
		description="Pan the camera left."
		check_type="Custom" allowed_in_modes="PhotoMode" >
		<keybind id="1" key="A" />
		<keybind id="2" key="LeftArrow" />
	</input_action>
	
	<input_action id="PhotoModeCameraRight" scope="MainGameOnly"
		category="PhotoModeCamera" display_name="Camera Right"
		description="Pan the camera right."
		check_type="Custom" allowed_in_modes="PhotoMode" >
		<keybind id="1" key="D" />
		<keybind id="2" key="RightArrow" />
	</input_action>

	<input_action id="PhotoModeCameraUp" scope="MainGameOnly"
		category="PhotoModeCamera" display_name="Camera Up"
		description="Pan the camera upwards."
		check_type="Custom" allowed_in_modes="PhotoMode" >
		<keybind id="1" key="E" />
		<keybind id="2" key="" />
	</input_action>

	<input_action id="PhotoModeCameraDown" scope="MainGameOnly"
		category="PhotoModeCamera" display_name="Camera Down"
		description="Pan the camera downwards."
		check_type="Custom" allowed_in_modes="PhotoMode">
		<keybind id="1" key="Q" />
		<keybind id="2" key="" />
	</input_action>
	
	<input_action id="PhotoModeCameraZoomInOutMouse" scope="MainGameOnly"
		category="PhotoModeCamera" display_name="Camera Zoom In/Out (Mouse)"
		description="Zoom the camera in and out with the mouse wheel."
		check_type="Custom" allowed_in_modes="PhotoMode" >
		<keybind id="1" key="MouseWheel" />
	</input_action>
	
	<!--Photo mode camera memory-->

