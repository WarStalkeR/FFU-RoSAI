	  dll_name="HotMExternalVisualizationCode" type_name="Arcen.HotM.ExternalVis.Input_MainUIHandler" check_type="ButtonPressStarting" allowed_in_modes="PhotoMode">
		<keybind id="1" modifier_1="LeftShift" key="Alpha1" />
		<keybind id="2" key="" />
	</input_action>
